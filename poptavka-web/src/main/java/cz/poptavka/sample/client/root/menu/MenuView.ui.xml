<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:with field='res'
		type='cz.poptavka.sample.client.resources.StyleResource' />

	<ui:style>
		.important {
			font-weight: bold;
		}
				
	</ui:style>
	<g:HTMLPanel>
		<div ui:field="homeMenu">
			<ul ui:field="menuList" class='{res.layout.homeMenu}'>
				<li>
					<g:Button ui:field="home">
						Home
						<!-- <ui:msg key="homeLink">Home</ui:msg> -->
					</g:Button>
				</li>
				<li>
					<g:Button ui:field="demands">
						Demands
						<!-- <ui:msg key="demands">Demands</ui:msg> -->
					</g:Button>
				</li>
				<li>
					<g:Button ui:field="suppliers">
						Suppliers
						<!-- <ui:msg key="suppliers">Suppliers</ui:msg> -->
					</g:Button>
				</li>
				<li>
					<g:Button ui:field="createSupplier">
						Create Supplier
						<!-- <ui:msg key="createSupplier">createSupplier</ui:msg> -->
					</g:Button>
				</li>
				<li>
					<g:Button ui:field="createDemand">
						Create Demand
						<!-- <ui:msg key="createDemand">createDemand</ui:msg> -->
					</g:Button>
				</li>
			</ul>
		</div>
	</g:HTMLPanel>
</ui:UiBinder>