<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
    xmlns:g="urn:import:com.google.gwt.user.client.ui">
    <ui:style>
        .important {
            font-weight: bold;
        }
    </ui:style>
    <g:HTMLPanel>
             <g:RadioButton name="type" ui:field="personRadio" ><ui:msg key="privatePerson">Private Person</ui:msg></g:RadioButton>
             <g:RadioButton name="type" ui:field="companyRadio" ><ui:msg key="company">Company</ui:msg></g:RadioButton>
             <g:Button ui:field="toLoginBtn"><ui:msg key="toLogin">To Login Page</ui:msg></g:Button>
        <g:SimplePanel ui:field="formHolder"/>
    </g:HTMLPanel>
</ui:UiBinder>
