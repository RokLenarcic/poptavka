<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
    xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:demands="urn:import:cz.poptavka.sample.client.user.demands">

    <ui:with field='res'
        type='cz.poptavka.sample.client.resources.StyleResource' />

    <g:TabLayoutPanel ui:field='tabLayoutPanel' barUnit="EM"
        barHeight="3">
        <g:tab>
            <g:header>
                <ui:msg key='demands'>Demands</ui:msg>
            </g:header>
            <g:SimplePanel ui:field="tabPanel">
                <g:HTML>TEST</g:HTML>
            </g:SimplePanel>
        </g:tab>
        <g:tab>
            <g:header>
                <em>
                    <ui:msg key='messages'>Messages</ui:msg>
                </em>
            </g:header>
            <g:HTML>
                <h1>Messages Tab</h1>
            </g:HTML>
        </g:tab>
        <g:tab>
            <g:header>
                <ui:msg key='settings'>Settings</ui:msg>
            </g:header>
            <g:HTML>
                <h1>Settings tab</h1>
            </g:HTML>
        </g:tab>
        <g:tab>
            <g:header>
                <ui:msg key='contacts'>Contacts</ui:msg>
            </g:header>
            <g:HTML>
                <h1>Contacts tab</h1>
            </g:HTML>
        </g:tab>
         <g:tab>
            <g:header>
                <ui:msg key='administration'>Admistration</ui:msg>
            </g:header>
            <g:HTML>
                <h1>Administration tab</h1>
            </g:HTML>
        </g:tab>
        <g:tab>
            <g:customHeader>
                <g:FlowPanel>
                    <g:TextBox>Search</g:TextBox>
                    <g:ListBox></g:ListBox>
                    <g:Button>Search</g:Button>
                </g:FlowPanel>
            </g:customHeader>
            <g:HTML>Custom header test</g:HTML>
        </g:tab>
    </g:TabLayoutPanel>
</ui:UiBinder>
