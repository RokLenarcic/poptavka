<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
    xmlns:g="urn:import:com.google.gwt.user.client.ui">
    <ui:style>
        .important {
            font-weight: bold;
            font-style: italic;
        }
        .block {
            display: block;
        }
    </ui:style>
    <g:HTMLPanel ui:field="detail" styleName="{style.block}">
            <table style="display: none;">
                <tr>
                    <td width="200">
                        <g:Label>
                            <ui:msg key="commonInfo">Common info: </ui:msg>
                        </g:Label>
                    </td>
                </tr>
                <tr>
                    <td width="200">
                        <g:Label>
                            <ui:msg key="demandName">Demand name: </ui:msg>
                        </g:Label>
                    </td>
                    <td>
                        <g:Label ui:field="demandName" />
                    </td>
                </tr>
                <tr>
                    <td>
                        <g:Label>
                            <ui:msg key="price">Price:</ui:msg>
                        </g:Label>
                    </td>
                    <td>
                        <g:Label ui:field="price" />
                    </td>
                </tr>
                <tr>
                    <td>
                        <g:Label>
                            <ui:msg key="endDate">End date:</ui:msg>
                        </g:Label>
                    </td>
                    <td>
                        <g:Label ui:field="endDate" />
                    </td>
                </tr>
                <tr>
                    <td>
                        <g:Label>
                            <ui:msg key="validTo">Valid to: </ui:msg>
                        </g:Label>
                    </td>
                    <td>
                        <g:Label ui:field="validTo" />
                    </td>
                </tr>
                <tr>
                    <td>
                        <g:Label>
                            <ui:msg key="type">Type: </ui:msg>
                        </g:Label>
                    </td>
                    <td>
                        <g:Label ui:field="type" />
                    </td>
                </tr>
                <tr>
                    <td width="200">
                        <g:Label>
                            <ui:msg key="categories">Categories: </ui:msg>
                        </g:Label>
                    </td>
                </tr>
                <tr>
                    <td>
                        <g:Label ui:field="categories" />
                    </td>
                </tr>
                <tr>
                    <td width="200">
                        <g:Label>
                            <ui:msg key="localities">Localities: </ui:msg>
                        </g:Label>
                    </td>
                </tr>
                <tr>
                    <td>
                        <g:Label ui:field="localities" />
                    </td>
                </tr>
                <tr>
                <tr>
                    <td width="200">
                        <g:Label>
                            <ui:msg key="detailInfo">Detail info: </ui:msg>
                        </g:Label>
                    </td>
                </tr>
                    <td>
                        <g:Label>
                            <ui:msg key="maxNumberOfSuppliers">Maximal number of suppliers:</ui:msg>
                        </g:Label>
                    </td>
                    <td>
                        <g:Label ui:field="maxNumberOfSuppliers" />
                    </td>
                </tr>
                <tr>
                    <td>
                        <g:Label>
                            <ui:msg key="minSupplierRating">Minimal supplier rating: </ui:msg>
                        </g:Label>
                    </td>
                    <td>
                        <g:Label ui:field="minSupplierRating" />
                    </td>
                </tr>
                <tr>
                    <td>
                        <g:Label>
                            <ui:msg key="excludedSuppliers">Excluded suppliers: </ui:msg>
                        </g:Label>
                    </td>
                    <td>
                        <g:Label ui:field="excludedSuppliers" />
                    </td>
                </tr>
                <tr>
                    <td>
                        <g:Label>
                            <ui:msg key="description">Description: </ui:msg>
                        </g:Label>
                    </td>
                    <td>
                        <g:Label ui:field="description" />
                    </td>
                </tr>
            </table>
    </g:HTMLPanel>
</ui:UiBinder>
