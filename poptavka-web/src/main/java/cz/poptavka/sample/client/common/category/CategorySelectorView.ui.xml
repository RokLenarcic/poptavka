            <g:Grid ui:field='categoryListHolder'>
                <g:row>
                    <g:customCell>
                        <g:HTML styleName='{res.cssBase.commonListLoader]'></g:HTML>
                    </g:customCell>
                    <g:customCell>
                        <g:HTML styleName='{res.cssBase.commonListLoader]'></g:HTML>
                    </g:customCell>
                    <g:customCell>
                        <g:HTML styleName='{res.cssBase.commonListLoader]'></g:HTML>
                    </g:customCell>
                    <g:customCell>
                        <g:HTML styleName='{res.cssBase.commonListLoader]'></g:HTML>
                    </g:customCell>
                    <g:customCell>
                        <g:HTML styleName='{res.cssBase.commonListLoader]'></g:HTML>
                    </g:customCell>
                    <g:customCell>
                        <g:HTML styleName='{res.cssBase.commonListLoader]'></g:HTML>
                    </g:customCell>

                </g:row>
            </g:Grid>
        </g:HorizontalPanel>
        <strong><ui:msg key="selectedCats">Selected</ui:msg>:</strong><br />
        <g:ListBox visibleItemCount="5" width="450" ui:field="selectedList"/>
    </g:HTMLPanel>
</ui:UiBinder>
