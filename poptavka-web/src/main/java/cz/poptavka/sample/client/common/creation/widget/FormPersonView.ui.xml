<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
    xmlns:g="urn:import:com.google.gwt.user.client.ui">

<g:Grid>
    <g:row>
        <g:customCell>
        <g:VerticalPanel>
        <g:HTML horizontalAlignment="ALIGN_CENTER">Login info</g:HTML>
        <g:Grid>
            <g:row>
                <g:customCell>
                    <g:HTML wordWrap="true">Email/Login <span style="color: red;">*</span></g:HTML>
                </g:customCell>
                <g:customCell>
                    <g:TextBox ui:field="mailBox">novy@user.cz</g:TextBox>
                </g:customCell>
            </g:row>
            <g:row>
                <g:customCell>
                    <g:HTML wordWrap="true">Heslo <span style="color: red;">*</span></g:HTML>
                </g:customCell>
                <g:customCell>
                    <g:PasswordTextBox ui:field="passBox">heslo</g:PasswordTextBox>
                </g:customCell>
            </g:row>
            <g:row>
                <g:customCell>
                    <g:HTML wordWrap="true">Heslo znovu</g:HTML>
                </g:customCell>
                <g:customCell>
                    <g:PasswordTextBox ui:field="passConfirmBox">heslo</g:PasswordTextBox>
                </g:customCell>
            </g:row>
        </g:Grid>
        </g:VerticalPanel>
        </g:customCell>
                <g:customCell>
        <g:VerticalPanel>
        <g:HTML wordWrap="true" horizontalAlignment="ALIGN_CENTER">Kontaktna osoba</g:HTML>
        <g:Grid>
            <g:row>
                <g:customCell>
                    <g:HTML wordWrap="true">Meno</g:HTML>
                </g:customCell>
                <g:customCell>
                    <g:TextBox ui:field="nameBox">Meno</g:TextBox>
                </g:customCell>
            </g:row>
            <g:row>
                <g:customCell>
                    <g:HTML wordWrap="true">Priezvisko</g:HTML>
                </g:customCell>
                <g:customCell>
                    <g:TextBox ui:field="surnameBox">Priezvisko</g:TextBox>
                </g:customCell>
            </g:row>
            <g:row>
                <g:customCell>
                    <g:HTML wordWrap="true">Telefon</g:HTML>
                </g:customCell>
                <g:customCell>
                    <g:TextBox ui:field="phoneBox">123456789</g:TextBox>
                </g:customCell>
            </g:row>
            <g:row>
                <g:customCell>
                    <g:HTML wordWrap="true">WWW</g:HTML>
                </g:customCell>
                <g:customCell>
                    <g:TextBox ui:field="websiteBox">www.sss.zzz</g:TextBox>
                </g:customCell>
            </g:row>
        </g:Grid>
        </g:VerticalPanel></g:customCell>
            </g:row>
            <g:row>
                <g:customCell>
        <g:VerticalPanel>
        <g:HTML wordWrap="true" horizontalAlignment="ALIGN_CENTER">Adresa </g:HTML>
        <g:Grid>
            <g:row>
                <g:customCell>
                    <g:HTML wordWrap="true">Ulica</g:HTML>
                </g:customCell>
                <g:customCell>
                    <g:TextBox ui:field="streetBox">ulica</g:TextBox>
                </g:customCell>
            </g:row>
            <g:row>
                <g:customCell>
                    <g:HTML wordWrap="true">Mesto</g:HTML>
                </g:customCell>
                <g:customCell>
                    <g:TextBox ui:field="cityBox">Brno</g:TextBox>
                </g:customCell>
            </g:row>
            <g:row>
                <g:customCell>
                    <g:HTML wordWrap="true">PSC</g:HTML>
                </g:customCell>
                <g:customCell>
                    <g:TextBox ui:field="zipBox">60200</g:TextBox>
                </g:customCell>
            </g:row>
        </g:Grid>
        </g:VerticalPanel>
        </g:customCell>
            </g:row>
    </g:Grid>
</ui:UiBinder>
