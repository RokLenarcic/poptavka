<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder' xmlns:g='urn:import:com.google.gwt.user.client.ui'>

    <ui:with field='res' type='cz.poptavka.sample.client.resources.StyleResource'/>

    <g:HTMLPanel>
        <g:ScrollPanel ui:field='masterPanel'>
            <g:Grid ui:field='categoryListHolder'>
                <g:row>
                    <g:customCell>
                    <g:HTML></g:HTML>
<!--                         <g:ListBox visibleItemCount='10' width="200"/> -->
                    </g:customCell>
                </g:row>
            </g:Grid>
        </g:ScrollPanel>
        <strong><ui:msg key="selectedCats">Selected</ui:msg>:</strong><br />
        <g:ListBox visibleItemCount="5" width="450" ui:field="selectedList"/>
    </g:HTMLPanel>
</ui:UiBinder>
