<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:d="urn:import:cz.poptavka.sample.client.home.demands">
	<ui:style>
		.name {
			font-weight: bold;
			width: auto;
		}
		.panel {
			width: 400px;
			spacing: 5;
		}
		.list {
			width: 100px;
		}
	</ui:style>
	<g:HTMLPanel>
		<g:VerticalPanel ui:field="verticalContent" >
			<g:HorizontalPanel styleName="{style.panel}" ui:field="myPanelContent" >
				<g:Label styleName="{style.name}" text="Demand Filters" />
				<g:ListBox styleName="{style.list}"/>
				<g:ListBox styleName="{style.list}"/>
			</g:HorizontalPanel>
			<!--  <d:DemandWidgetBinder ui:field="demand" /> -->
		</g:VerticalPanel>
	</g:HTMLPanel>
</ui:UiBinder> 