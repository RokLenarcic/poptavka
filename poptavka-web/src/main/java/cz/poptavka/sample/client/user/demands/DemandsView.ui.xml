	<g:HTMLPanel>
		<g:HorizontalPanel>
			<g:HTML wordWrap="true" styleName='{style.menuBar}'>MENU PLACEHOLDER</g:HTML>
			<g:SplitLayoutPanel styleName='{style.contentPanel}'>
				<g:north size='200'>
					<g:HorizontalPanel styleName='{style.headerPanel}'>
						<g:HTML>
							<h1>Moje Poptávky</h1>
						</g:HTML>
						<g:Button ui:field="answerButton">Odpovedať</g:Button>
						<g:Button ui:field="editButton">Upraviť</g:Button>
						<g:Button ui:field="closeButton">Zavrieť</g:Button>
						<g:Button ui:field="cancelButton">Zrušiť</g:Button>
						<g:ListBox ui:field="actionSelectButton" />
					</g:HorizontalPanel>
				</g:north>

				<g:center size='200'>
					<g:HorizontalPanel>
						<g:FlexTable ui:field="demandsTable"></g:FlexTable>
					</g:HorizontalPanel>
				</g:center>
			</g:SplitLayoutPanel>

		</g:HorizontalPanel>
