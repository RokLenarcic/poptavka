	<ui:style>
		.menuBar {
			margin-left: 10px;
			width: 200px;
			height: 100%;
			border: 1px solid black;
		}

		.contentPanel {
			margin: 0 auto;
			border: 1px solid black;
		}

		.headerPanel {
			height: 200px;
			width: 800px;
		}

		h1 {
			margin: 4px;
		}
	</ui:style>
