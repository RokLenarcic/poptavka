<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
    <g:HTMLPanel ui:field="masterLayout">
        <g:SimplePanel ui:field="header" />
        <g:DecoratorPanel>
            <g:HorizontalPanel>
                <g:SimplePanel ui:field="menu" />
                <g:SimplePanel ui:field="searchBar" />
            </g:HorizontalPanel>
        </g:DecoratorPanel>
        <g:SimplePanel ui:field="body" styleName="body" />
        <g:SimplePanel ui:field="footer" styleName="body" />
    </g:HTMLPanel>
</ui:UiBinder>