<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:g="urn:import:com.google.gwt.user.client.ui"
             xmlns:b="urn:import:com.github.gwtbootstrap.client.ui">

    <g:HTMLPanel>
        <b:Modal ui:field="showErrorModal" backdrop="STATIC" keyboard="true" animation="true">
            <g:Label ui:field="showErrorLabel" />
            <b:ModalFooter ui:field="showErrorFooter">
                <b:Button ui:field="showErrorCloseBtn" icon="OFF" type="PRIMARY">
                    <ui:msg key="commonBtnClose">Close</ui:msg>
                </b:Button>
            </b:ModalFooter>
        </b:Modal>
    </g:HTMLPanel>
</ui:UiBinder>
