<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:g="urn:import:com.google.gwt.user.client.ui">

    <ui:with field='res' type='com.eprovement.poptavka.resources.StyleResource' />

    <g:HTMLPanel ui:field="menuPanel" styleName='{res.header.menu}'>
        <g:Button ui:field="menuOpenButton" styleName='menu-open-button'></g:Button>
        <ul>
            <li>
                <g:Button ui:field="home" styleName='button1'>
                    <ui:msg key="menuHome">Home</ui:msg>
                </g:Button>
            </li>
            <li>
                <g:Button ui:field="client" styleName='button6'>
                    <ui:msg key="menuClient">Client</ui:msg>
                </g:Button>
            </li>
            <li>
                <g:Button ui:field="supplier" styleName='button9'>
                    <ui:msg key="menuSupplier">Professional</ui:msg>
                </g:Button>
            </li>
            <li>
                <g:Button ui:field="inbox" styleName='button7'>
                    <ui:msg key='menuMessages'>Inbox</ui:msg>
                </g:Button>
            </li>
            <li>
                <g:Button ui:field="demands" styleName='button2'>
                    <ui:msg key="menuDemands">Projects</ui:msg>
                </g:Button>
            </li>
            <li>
                <g:Button ui:field="createDemand"  styleName='button3'>
                    <ui:msg key="createDemand">Post Project</ui:msg>
                </g:Button>
            </li>
            <li>
                <g:Button ui:field="suppliers" styleName='button4'>
                    <ui:msg key="menuSuppliers">Professionals</ui:msg>
                </g:Button>
            </li>
            <li>
                <g:Button ui:field="createSupplier"  styleName='button5'>
                    <ui:msg key="menucreateSupplier">Become Professional</ui:msg>
                </g:Button>
            </li>
            <li>
                <g:Button ui:field="administration"  styleName='button8'>
                    <ui:msg key='menuAdministration'>Admistration</ui:msg>
                </g:Button>
            </li>
        </ul>
        <g:HTMLPanel ui:field="userMenuPanel" styleName='{res.header.userResponsiveMenu}'>
            <g:Label ui:field="userLabel" styleName='{res.header.userLabel}'>hello@gmail.com</g:Label>
            <g:Button ui:field="userSettings" styleName='{res.header.userSettings}'>
                <ui:msg key="userHeaderMyProfile">My Profile</ui:msg>
            </g:Button>
            <g:Button ui:field="userLogout" styleName='{res.header.userLogout}'>
                <ui:msg key="userHeaderLogOut">Log Out</ui:msg>
            </g:Button>
        </g:HTMLPanel>
    </g:HTMLPanel>
</ui:UiBinder>
