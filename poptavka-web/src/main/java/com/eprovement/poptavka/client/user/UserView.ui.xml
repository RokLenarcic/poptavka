<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<!-- xmlns:demands="urn:import:com.eprovement.poptavka.client.user.demands"> -->

	<ui:with field='res'
		type='com.eprovement.poptavka.client.resources.StyleResource' />

	<ui:style>
		.important {
			font-weight: bold;
		}
		
		.background {
			background-color: red;
		}
	</ui:style>

	<g:HorizontalPanel>
		<g:HTMLPanel styleName="style.background">
			<div ui:field="homeMenu">
				<ul ui:field="menuList">
					<li>
						<g:Button ui:field="demands" styleName='{res.layout.homeMenu}'>
							<!-- <ui:msg key="demands">Demands</ui:msg> -->
							Demands
						</g:Button>
					</li>
					<li>
						<g:Button ui:field="messages" styleName='{res.layout.homeMenu}'>
							<!-- <ui:msg key='messages'>Messages</ui:msg> -->
							Messages
						</g:Button>
					</li>
					<li>
						<g:Button ui:field="settings" styleName='{res.layout.homeMenu}'>
							<!-- <ui:msg key='settings'>Settings</ui:msg> -->
							Settings
						</g:Button>
					</li>
					<li>
						<g:Button ui:field="contacts" styleName='{res.layout.homeMenu}'>
							<!-- <ui:msg key='contacts'>Contacts</ui:msg> -->
							Contacts
						</g:Button>
					</li>
					<li>
						<g:Button ui:field="administration" styleName='{res.layout.homeMenu}'>
							<!-- <ui:msg key='administration'>Admistration</ui:msg> -->
							Admistration
						</g:Button>
					</li>
				</ul>
			</div>
		</g:HTMLPanel>
	</g:HorizontalPanel>
</ui:UiBinder>
