<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:cm="urn:import:com.eprovement.poptavka.client.common"
        xmlns:g='urn:import:com.google.gwt.user.client.ui'>

    <ui:with field='res' type='com.eprovement.poptavka.client.resources.StyleResource' />
    <ui:with field='text' type='com.google.gwt.i18n.client.LocalizableMessages' />

    <ui:style>


    </ui:style>

    <g:DockLayoutPanel ui:field="layoutMaster" unit="PX">
        <g:north size="148">
            <g:HTMLPanel ui:field='headerHolder' styleName='{res.layout.headerContainer}'>
                <g:SimplePanel ui:field='header'/>
                <g:HorizontalPanel>
                    <g:SimplePanel ui:field="menu" />
                    <g:SimplePanel ui:field="searchBar" />
                </g:HorizontalPanel>
            </g:HTMLPanel>
        </g:north>
        <g:south size="38">
            <g:SimplePanel ui:field="footer"/>
        </g:south>
        <g:center>
            <g:SimplePanel ui:field='body' />
        </g:center>
    </g:DockLayoutPanel>
</ui:UiBinder>