<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:g="urn:import:com.google.gwt.user.client.ui">
    <ui:with field='res'
             type='com.eprovement.poptavka.client.resources.StyleResource' />

    <g:HTMLPanel>
        <div ui:field="homeMenu">
            <ul ui:field="menuList" class='{res.layout.homeMenu}'>
                <li>
                    <g:Button ui:field="home">
                        <ui:msg key="menuHome">Home</ui:msg>
                    </g:Button>
                </li>
                <li>
                    <g:Button ui:field="demands">
                        <ui:msg key="menuHomeDemands">Demands</ui:msg>
                    </g:Button>
                </li>
                <li>
                    <g:Button ui:field="suppliers">
                        <ui:msg key="menuHomeSuppliers">Suppliers</ui:msg>
                    </g:Button>
                </li>
                <li>
                    <g:Button ui:field="createSupplier">
                        <ui:msg key="menuCreateSupplier">createSupplier</ui:msg>
                    </g:Button>
                </li>
                <li>
                    <g:Button ui:field="createDemand">
                        <ui:msg key="menuCreateDemand">createDemand</ui:msg>
                    </g:Button>
                </li>
            </ul>
        </div>
    </g:HTMLPanel>
</ui:UiBinder>