<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:b="urn:import:com.github.gwtbootstrap.client.ui"
             xmlns:bb="urn:import:com.github.gwtbootstrap.client.ui.base"
             xmlns:g="urn:import:com.google.gwt.user.client.ui">

    <ui:with field='res' type='com.eprovement.poptavka.resources.StyleResource' />

    <g:HTMLPanel>
        <b:Tooltip text="Menu" placement="RIGHT">
            <bb:IconAnchor ui:field='menuAnchor' icon="LIST_UL" iconSize="TWO_TIMES" styleName='{res.header.menuIcon}'/>
        </b:Tooltip>
        <g:SimplePanel ui:field="menuPanel" styleName='{res.header.menuPanel}'/>
    </g:HTMLPanel>
</ui:UiBinder>