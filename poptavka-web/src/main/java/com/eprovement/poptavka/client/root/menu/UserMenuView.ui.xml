<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">

	<ui:with field='res'
		type='com.eprovement.poptavka.resources.StyleResource' />

	<ui:style>
		.button1 { background: url(images/menu-icons.png) no-repeat 0 0; }
		.button2 { background: url(images/menu-icons.png) no-repeat 0 -90px; }
		.button3 { background: url(images/menu-icons.png) no-repeat 0 -180px; }
		.button4 { background: url(images/menu-icons.png) no-repeat 0 -270px; }
		.button5 { background: url(images/menu-icons.png) no-repeat 0 -360px; }
		.button6 { background: url(images/menu-icons.png) no-repeat 0 -450px; }
		.button7 { background: url(images/menu-icons.png) no-repeat 0 -540px; }
		.button8 { background: url(images/menu-icons.png) no-repeat 0 -630px; }
		.button9 { background: url(images/menu-icons.png) no-repeat 0 -720px; }

		.button1:hover { background: url(images/menu-icons-hover.png) no-repeat 0 0; }
		.button2:hover { background: url(images/menu-icons-hover.png) no-repeat 0 -90px; }
		.button3:hover { background: url(images/menu-icons-hover.png) no-repeat 0 -180px; }
		.button4:hover { background: url(images/menu-icons-hover.png) no-repeat 0 -270px; }
		.button5:hover { background: url(images/menu-icons-hover.png) no-repeat 0 -360px; }
		.button6:hover { background: url(images/menu-icons-hover.png) no-repeat 0 -450px; }
		.button7:hover { background: url(images/menu-icons-hover.png) no-repeat 0 -540px; }
		.button8:hover { background: url(images/menu-icons-hover.png) no-repeat 0 -630px; }
		.button9:hover { background: url(images/menu-icons-hover.png) no-repeat 0 -720px; }
	</ui:style>

	<g:HTMLPanel>
		<div ui:field="homeMenu">
			<ul ui:field="menuList">
				<li>
					<g:Button ui:field="client" styleName='{style.button6}'>
						<ui:msg key="menuClient">Client</ui:msg><br /><br />
					</g:Button>
				</li>
				<li>
					<g:Button ui:field="supplier" styleName='{style.button7}'>
						<ui:msg key="menuSupplier">Supplier</ui:msg><br /><br />
					</g:Button>
				</li>
				<li>
					<g:Button ui:field="inbox" styleName='{style.button8}'>
						<ui:msg key='menuMessages'>Messages</ui:msg><br /><br />
					</g:Button>
				</li>
				<li>
					<g:Button ui:field="demands" styleName='{style.button2}'>
						<ui:msg key="menuDemands">Demands</ui:msg><br /><br />
					</g:Button>
				</li>
                <li>
					<g:Button ui:field="createDemand"  styleName='{style.button3}'>
						<ui:msg key="createDemand">Create Demand</ui:msg>
					</g:Button>
				</li>
				<li>
					<g:Button ui:field="suppliers" styleName='{style.button4}'>
						<ui:msg key="menuSuppliers">Suppliers</ui:msg><br /><br />
					</g:Button>
				</li>
                <li>
					<g:Button ui:field="createSupplier"  styleName='{style.button5}'>
						<ui:msg key="menucreateSupplier">Create supplier</ui:msg>
					</g:Button>
				</li>
				<li>
					<g:Button ui:field="administration"  styleName='{style.button8}'>
						<ui:msg key='menuAdministration'>Admistration</ui:msg><br /><br />
					</g:Button>
				</li>
			</ul>
		</div>
	</g:HTMLPanel>
</ui:UiBinder>
