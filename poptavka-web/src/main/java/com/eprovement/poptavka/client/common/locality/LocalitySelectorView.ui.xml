<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'
             xmlns:c='urn:import:com.google.gwt.user.cellview.client'>

    <ui:with field='res' type='com.eprovement.poptavka.resources.StyleResource'/>

    <g:DockLayoutPanel unit="PX" styleName='well well-container'>
        <g:north size="15">
            <g:HTMLPanel></g:HTMLPanel>
        </g:north>
        <g:west size="15">
            <g:HTMLPanel></g:HTMLPanel>
        </g:west>
        <g:center>
            <g:HTMLPanel styleName='pull-left'>
                <g:SimplePanel ui:field="cellBrowserHolder"/>
                <g:HTML ui:field='loader' visible='false' styleName='{res.common.smallLoader}' />
            </g:HTMLPanel>
        </g:center>
        <g:south size="40">
            <g:HTMLPanel styleName='{res.common.selectedItemsWidget}'>
                <g:Label ui:field="selectedCountLabel" styleName="selected-Label">
                    <ui:msg key="commonSelected" >Selected</ui:msg>:
                </g:Label>
                <g:ScrollPanel styleName='scroll-Container'>
                    <c:CellList ui:field="cellList"/>
                </g:ScrollPanel>
            </g:HTMLPanel>
        </g:south>
        <g:east size="15">
            <g:HTMLPanel></g:HTMLPanel>
        </g:east>
        <g:south size="10">
            <g:HTMLPanel></g:HTMLPanel>
        </g:south>
    </g:DockLayoutPanel>
</ui:UiBinder>
