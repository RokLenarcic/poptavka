<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'
             xmlns:c='urn:import:com.google.gwt.user.cellview.client'>

    <ui:with field='res' type='com.eprovement.poptavka.resources.StyleResource'/>

    <g:VerticalPanel>
        <g:HorizontalPanel>
            <g:SimplePanel ui:field="cellBrowserHolder"/>
            <g:HTML ui:field='loader' visible='false' styleName='{res.common.smallLoader}' />
        </g:HorizontalPanel>
        <g:HorizontalPanel>
            <g:VerticalPanel>
                <g:Label>
                    <ui:msg key="commonSelected">Selected</ui:msg>:
                </g:Label>
                <c:CellList ui:field="cellList"/>
            </g:VerticalPanel>
        </g:HorizontalPanel>
    </g:VerticalPanel>
</ui:UiBinder>
